class Solution {
        
    public int reverse(int x) {
        long rem,rev=0;
        while(x!=0){
            rem=x%10;
            rev=(rev*10)+ rem;
            x=x/10;
        }
        if(rev>Integer. MAX_VALUE-1 || rev<Integer.MIN_VALUE) return 0;
        
        else return (int)rev;
    }
}


https://leetcode.com/problems/reverse-integer/
